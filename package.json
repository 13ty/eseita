{
      "name": "essay-generator-pro",
      "private": true,
      "version": "1.0.0",
      "type": "module",
      "scripts": {
        "dev": "vite",
        "build": "vite build",
        "preview": "vite preview",
        "lint": "eslint src --ext js,jsx --report-unused-disable-directives --max-warnings 0",
        "format": "prettier --write \"src/**/*.{js,jsx}\"",
        "prepare": "husky install"
      },
      "dependencies": {
        "@sentry/react": "^7.54.0",
        "axios": "^1.4.0",
        "file-saver": "^2.0.5",
        "jspdf": "^2.5.1",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-error-boundary": "^4.0.10",
        "react-icons": "^4.10.1",
        "react-modal": "^3.16.1"
      },
      "devDependencies": {
        "@types/react": "^18.0.28",
        "@types/react-dom": "^18.0.11",
        "@vitejs/plugin-react": "^4.0.3",
        "eslint": "^8.38.0",
        "eslint-config-react-app": "^7.0.1",
        "husky": "^8.0.3",
        "lint-staged": "^13.2.2",
        "prettier": "^2.8.8",
        "rollup-plugin-visualizer": "^5.9.0",
        "vite": "^4.3.9",
        "vite-plugin-compression2": "^0.1.17"
      },
      "eslintConfig": {
        "extends": "react-app",
        "rules": {
          "no-console": "warn"
        }
      },
      "lint-staged": {
        "src/**/*.{js,jsx}": [
          "eslint",
          "prettier --write"
        ]
      }
    }
